{
    "name": "divido/merchant-sdk",
    "type": "library",
    "authors": [
        {
            "name": "Neil McGibbon",
            "email": "neil.mcgibbon@divido.com"
        },
        {
            "name": "Mike Lovely",
            "email": "mike.lovely@divido.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Divido\\MerchantSDK\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Divido\\MerchantSDK\\Test\\": "tests/"
        },
        "classmap": [
            "tests"
        ]
    },
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.7",
        "php-http/message": "^1.8",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1"
    },
    "require-dev": {
        "php-http/mock-client": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "bin-dir": "bin/"
    }
}
