{
    "name": "divido/merchant-sdk",
    "type": "library",
    "authors": [
        {
            "name": "Neil McGibbon",
            "email": "neil.mcgibbon@divido.com"
        },
        {
            "name": "Mike Lovely",
            "email": "mike.lovely@divido.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Divido\\MerchantSDK\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Divido\\MerchantSDK\\Test\\Unit\\": "tests/unit/",
            "Divido\\MerchantSDK\\Test\\Integration\\": "tests/integration/"
        },
        "classmap": [
            "tests"
        ]
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "php-http/discovery": "^1.7",
        "php-http/message": "^1.8",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1"
    },
    "require-dev": {
        "php-http/mock-client": "^1.0",
        "phpunit/phpunit": "^9.3",
        "nyholm/psr7": "^1.8"
    },
    "config": {
        "bin-dir": "bin/",
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
