{
    "name": "divido/merchant-sdk",
    "type": "library",
    "authors": [
        {
            "name": "Neil McGibbon",
            "email": "code@neilmcgibbon.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:DividoFinancialServices/php-error-lib.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Divido\\MerchantSDK\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Divido\\MerchantSDK\\Test\\": "tests/"
        },
        "classmap": [
            "tests"
        ]
    },
    "require": {
        "php": ">=7",
        "psr/log": "^1",
        "guzzlehttp/guzzle": "^6.2",
        "enm/json-api-client": "^1.2",
        "divido/php-error-lib": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "kint-php/kint": "^2.1",
        "hamcrest/hamcrest-php": "^2.0",
        "mockery/mockery": "1.1.0",
        "phpdocumentor/reflection-docblock": "~2.0",
        "phpdocumentor/phpdocumentor": "2.9.0"
    }
}
